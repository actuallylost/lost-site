{
	"extends": [
		"next/core-web-vitals",
		"eslint:recommended",
		"plugin:@typescript-eslint/recommended",
		"prettier"
	],
	"plugins": ["@typescript-eslint"],
	"rules": {
		"semi": ["error", "always"],
		"quotes": ["error", "double"],
		"indent": ["error", "tab"],
		"linebreak-style": ["error", "unix"]
	}
}
